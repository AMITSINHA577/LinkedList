package linkedList;

public class TestingAllCode {

	linkNode<Integer> head;

	public void headinsert(int data) {
		linkNode<Integer> newNode = new linkNode<Integer>(data);

		if (head == null) {
			head = newNode;
			return;
		}

		newNode.next = head;
		head = newNode;
	}
	
	
	public void addAnyPlce(int index, int data) {
		linkNode<Integer> newNode = new linkNode(data);
		
		if(index == 0) {
			newNode.next = head;
			head = newNode;
			return;
		}
		
		int currentIndex=  0;
		linkNode<Integer> temp = head;
		
		while(temp!=null && currentIndex<index-1) {
			currentIndex++;
			temp = temp.next;
		}
		
		if(temp == null) {
			System.out.println("worng index");
			return;
		}
		
		newNode.next = temp.next;
		temp.next = newNode;
	}

	public void printData() {
		linkNode<Integer> temp = head;
		while (temp != null) {
			System.out.println(temp.data);
			temp = temp.next;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		TestingAllCode obj1 = new TestingAllCode();
		obj1.headinsert(10);
		obj1.headinsert(30);
		obj1.headinsert(90);
		obj1.headinsert(70);
//		obj1.printData();
		
		obj1.addAnyPlce(0, 50);
		obj1.addAnyPlce(2, 40);
//		obj1.addAnyPlce(10, 530);

		obj1.printData();

	}

}

class linkNode<T> {
	T data;
	linkNode<T> next;

	public linkNode() {
		// TODO Auto-generated constructor stub
		this.next = null;
	}

	public linkNode(T data) {
		this.data = data;
		this.next = null;
	}
}