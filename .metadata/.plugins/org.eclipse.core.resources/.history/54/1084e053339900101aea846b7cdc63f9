package linkedList;

public class InsertInToHead {

	Node<Integer> head;

	public void insertHead(int data) {

		Node<Integer> newNode = new Node<Integer>(data);
		if (head == null) {
			head = newNode;
		}

		newNode.next = head;
		head = newNode;

	}

	public void printListedlist() {
		Node<Integer> temp = head;
		while (temp != null) {
			System.out.println(temp.next);
			temp = temp.next;
		}
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		InsertInToHead obj1 = new InsertInToHead();
		obj1.insertHead(10);
		obj1.insertHead(30);
		obj1.insertHead(70);
		obj1.insertHead(40);
		obj1.printListedlist();

	}

}

class Node<T> {
	T data;
	Node next;

	Node() {
		this.next = null;
	}

	Node(T data) {
		this.data = data;
		this.next = null;
	}
}